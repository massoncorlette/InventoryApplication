<div id="sidebarContainer">
  <div id="byGenre">
    <h2>Genre</h2>
    <% genres.forEach((genre, index) => { %>
    <div class="sidebarCells">
      <%- include('sidebargenre', {
        genre: genre, 
        index: index,
      }) %>
    </div>
    <% }) %>
  </div>
  <div id="byDirector">
    <h2>Director</h2>
    <% directors.forEach((director, index) => { %> 
      <div class="sidebarCells">
        <%- include('sidebardirector', {
          director: director,
          index: index,
        }) %>
      </div>
    <% }) %>        
  </div>
</div>